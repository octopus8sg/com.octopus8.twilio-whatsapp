<?xml version="1.0"?>
<extension key="twilio_whatsapp" type="module">
  <file>twilio_whatsapp</file>
  <name>twilio_whatsapp</name>
  <description>This is an extension that allows broadcasting of messages through Twilio Whatsapp API</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Thai Siang</author>
    <email>thaisiang@octopus8.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-07-22</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.75</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/TwilioWhatsapp</namespace>
    <format>23.02.1</format>
    <angularModule>crmTwilioWhatsapp</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
